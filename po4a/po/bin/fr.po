msgstr "Entité d'inclusion du prologue %%%s; non reconnue"

#: ../../lib/Locale/Po4a/Sgml.pm:768
#, perl-format
msgid "Can't close tempfile: %s"
msgstr "Impossible de fermer le fichier temporaire : %s"

#: ../../lib/Locale/Po4a/Sgml.pm:774
#, perl-format
msgid "Can't run nsgmls: %s"
msgstr "Impossible d'exécuter nsgmls : %s"

#: ../../lib/Locale/Po4a/Sgml.pm:885
#, perl-format
